{
  "name": "scrumworks/enum-mapping",
  "description": "Context based enum mapping",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "symfony/serializer": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "ScrumWorks\\EnumMapping\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "php -d memory_limit=2048M vendor/bin/phpstan.phar analyse --level max --ansi src"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpstan/phpstan": "^1.10"
  }
}
